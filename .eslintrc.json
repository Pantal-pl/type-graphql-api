{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "camelcase": "off",
    "no-shadow": "off"
  }
}
